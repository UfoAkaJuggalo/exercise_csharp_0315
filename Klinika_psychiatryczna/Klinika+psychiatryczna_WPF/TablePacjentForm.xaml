<Window x:Class="Klinika_psychiatryczna_WPF.TablePacjentForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klinika_psychiatryczna_WPF"
        mc:Ignorable="d"
        Title="Pacjenci kliniki" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <DataGrid HorizontalAlignment="Left" VerticalAlignment="Top" ItemsSource="{Binding viewModel.Pacjenci}"/>
        

    </Grid>
</Window>
